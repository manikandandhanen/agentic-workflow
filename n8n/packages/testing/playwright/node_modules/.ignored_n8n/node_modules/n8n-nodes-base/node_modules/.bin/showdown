#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/manikandan-d/n8/n8n/node_modules/.pnpm/showdown@2.1.0/node_modules/showdown/bin/node_modules:/home/manikandan-d/n8/n8n/node_modules/.pnpm/showdown@2.1.0/node_modules/showdown/node_modules:/home/manikandan-d/n8/n8n/node_modules/.pnpm/showdown@2.1.0/node_modules:/home/manikandan-d/n8/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/manikandan-d/n8/n8n/node_modules/.pnpm/showdown@2.1.0/node_modules/showdown/bin/node_modules:/home/manikandan-d/n8/n8n/node_modules/.pnpm/showdown@2.1.0/node_modules/showdown/node_modules:/home/manikandan-d/n8/n8n/node_modules/.pnpm/showdown@2.1.0/node_modules:/home/manikandan-d/n8/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../showdown/bin/showdown.js" "$@"
else
  exec node  "$basedir/../showdown/bin/showdown.js" "$@"
fi

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/manikandan-d/n8/n8n/node_modules/.pnpm/vitest@3.1.3_@types+debug@4.1.12_@types+node@20.19.21_jiti@2.6.1_jsdom@23.0.1_bufferuti_c9312c8ab0039d3967d9bd06f4524652/node_modules/vitest/node_modules:/home/manikandan-d/n8/n8n/node_modules/.pnpm/vitest@3.1.3_@types+debug@4.1.12_@types+node@20.19.21_jiti@2.6.1_jsdom@23.0.1_bufferuti_c9312c8ab0039d3967d9bd06f4524652/node_modules:/home/manikandan-d/n8/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/manikandan-d/n8/n8n/node_modules/.pnpm/vitest@3.1.3_@types+debug@4.1.12_@types+node@20.19.21_jiti@2.6.1_jsdom@23.0.1_bufferuti_c9312c8ab0039d3967d9bd06f4524652/node_modules/vitest/node_modules:/home/manikandan-d/n8/n8n/node_modules/.pnpm/vitest@3.1.3_@types+debug@4.1.12_@types+node@20.19.21_jiti@2.6.1_jsdom@23.0.1_bufferuti_c9312c8ab0039d3967d9bd06f4524652/node_modules:/home/manikandan-d/n8/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vitest/vitest.mjs" "$@"
else
  exec node  "$basedir/../vitest/vitest.mjs" "$@"
fi

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/manikandan-d/agentic-mcp/n8n/node_modules/.pnpm/@sentry+cli@2.55.0_encoding@0.1.13/node_modules/@sentry/cli/bin/node_modules:/home/manikandan-d/agentic-mcp/n8n/node_modules/.pnpm/@sentry+cli@2.55.0_encoding@0.1.13/node_modules/@sentry/cli/node_modules:/home/manikandan-d/agentic-mcp/n8n/node_modules/.pnpm/@sentry+cli@2.55.0_encoding@0.1.13/node_modules/@sentry/node_modules:/home/manikandan-d/agentic-mcp/n8n/node_modules/.pnpm/@sentry+cli@2.55.0_encoding@0.1.13/node_modules:/home/manikandan-d/agentic-mcp/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/manikandan-d/agentic-mcp/n8n/node_modules/.pnpm/@sentry+cli@2.55.0_encoding@0.1.13/node_modules/@sentry/cli/bin/node_modules:/home/manikandan-d/agentic-mcp/n8n/node_modules/.pnpm/@sentry+cli@2.55.0_encoding@0.1.13/node_modules/@sentry/cli/node_modules:/home/manikandan-d/agentic-mcp/n8n/node_modules/.pnpm/@sentry+cli@2.55.0_encoding@0.1.13/node_modules/@sentry/node_modules:/home/manikandan-d/agentic-mcp/n8n/node_modules/.pnpm/@sentry+cli@2.55.0_encoding@0.1.13/node_modules:/home/manikandan-d/agentic-mcp/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@sentry+cli@2.55.0_encoding@0.1.13/node_modules/@sentry/cli/bin/sentry-cli" "$@"
else
  exec node  "$basedir/../../../../../../@sentry+cli@2.55.0_encoding@0.1.13/node_modules/@sentry/cli/bin/sentry-cli" "$@"
fi

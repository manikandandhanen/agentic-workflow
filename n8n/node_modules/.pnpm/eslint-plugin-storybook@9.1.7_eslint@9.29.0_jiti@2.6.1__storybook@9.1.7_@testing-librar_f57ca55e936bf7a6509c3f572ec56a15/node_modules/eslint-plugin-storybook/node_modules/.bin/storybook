#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/manikandan-d/agentic-mcp/n8n/node_modules/.pnpm/storybook@9.1.7_@testing-library+dom@10.4.0_bufferutil@4.0.9_prettier@3.6.2_utf-8-valid_1fb71800f41a92c96d6d4a22f4f612cf/node_modules/storybook/bin/node_modules:/home/manikandan-d/agentic-mcp/n8n/node_modules/.pnpm/storybook@9.1.7_@testing-library+dom@10.4.0_bufferutil@4.0.9_prettier@3.6.2_utf-8-valid_1fb71800f41a92c96d6d4a22f4f612cf/node_modules/storybook/node_modules:/home/manikandan-d/agentic-mcp/n8n/node_modules/.pnpm/storybook@9.1.7_@testing-library+dom@10.4.0_bufferutil@4.0.9_prettier@3.6.2_utf-8-valid_1fb71800f41a92c96d6d4a22f4f612cf/node_modules:/home/manikandan-d/agentic-mcp/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/manikandan-d/agentic-mcp/n8n/node_modules/.pnpm/storybook@9.1.7_@testing-library+dom@10.4.0_bufferutil@4.0.9_prettier@3.6.2_utf-8-valid_1fb71800f41a92c96d6d4a22f4f612cf/node_modules/storybook/bin/node_modules:/home/manikandan-d/agentic-mcp/n8n/node_modules/.pnpm/storybook@9.1.7_@testing-library+dom@10.4.0_bufferutil@4.0.9_prettier@3.6.2_utf-8-valid_1fb71800f41a92c96d6d4a22f4f612cf/node_modules/storybook/node_modules:/home/manikandan-d/agentic-mcp/n8n/node_modules/.pnpm/storybook@9.1.7_@testing-library+dom@10.4.0_bufferutil@4.0.9_prettier@3.6.2_utf-8-valid_1fb71800f41a92c96d6d4a22f4f612cf/node_modules:/home/manikandan-d/agentic-mcp/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../storybook@9.1.7_@testing-library+dom@10.4.0_bufferutil@4.0.9_prettier@3.6.2_utf-8-valid_1fb71800f41a92c96d6d4a22f4f612cf/node_modules/storybook/bin/index.cjs" "$@"
else
  exec node  "$basedir/../../../../../storybook@9.1.7_@testing-library+dom@10.4.0_bufferutil@4.0.9_prettier@3.6.2_utf-8-valid_1fb71800f41a92c96d6d4a22f4f612cf/node_modules/storybook/bin/index.cjs" "$@"
fi

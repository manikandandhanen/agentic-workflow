#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/manikandan-d/miq_prd_hub/n8n/node_modules/.pnpm/tsdown@0.15.6_typescript@5.9.2_vue-tsc@2.2.8_patch_hash=e2aee939ccac8a57fe449bfd92bedd8_b8ee7384527e478f50e207c7bd4894d4/node_modules/tsdown/dist/node_modules:/home/manikandan-d/miq_prd_hub/n8n/node_modules/.pnpm/tsdown@0.15.6_typescript@5.9.2_vue-tsc@2.2.8_patch_hash=e2aee939ccac8a57fe449bfd92bedd8_b8ee7384527e478f50e207c7bd4894d4/node_modules/tsdown/node_modules:/home/manikandan-d/miq_prd_hub/n8n/node_modules/.pnpm/tsdown@0.15.6_typescript@5.9.2_vue-tsc@2.2.8_patch_hash=e2aee939ccac8a57fe449bfd92bedd8_b8ee7384527e478f50e207c7bd4894d4/node_modules:/home/manikandan-d/miq_prd_hub/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/manikandan-d/miq_prd_hub/n8n/node_modules/.pnpm/tsdown@0.15.6_typescript@5.9.2_vue-tsc@2.2.8_patch_hash=e2aee939ccac8a57fe449bfd92bedd8_b8ee7384527e478f50e207c7bd4894d4/node_modules/tsdown/dist/node_modules:/home/manikandan-d/miq_prd_hub/n8n/node_modules/.pnpm/tsdown@0.15.6_typescript@5.9.2_vue-tsc@2.2.8_patch_hash=e2aee939ccac8a57fe449bfd92bedd8_b8ee7384527e478f50e207c7bd4894d4/node_modules/tsdown/node_modules:/home/manikandan-d/miq_prd_hub/n8n/node_modules/.pnpm/tsdown@0.15.6_typescript@5.9.2_vue-tsc@2.2.8_patch_hash=e2aee939ccac8a57fe449bfd92bedd8_b8ee7384527e478f50e207c7bd4894d4/node_modules:/home/manikandan-d/miq_prd_hub/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../tsdown/dist/run.mjs" "$@"
else
  exec node  "$basedir/../tsdown/dist/run.mjs" "$@"
fi

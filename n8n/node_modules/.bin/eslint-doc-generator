#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/manikandan-d/miq_prd_hub/n8n/node_modules/.pnpm/eslint-doc-generator@2.2.2_eslint@9.29.0_jiti@2.6.1__typescript@5.9.2/node_modules/eslint-doc-generator/dist/bin/node_modules:/home/manikandan-d/miq_prd_hub/n8n/node_modules/.pnpm/eslint-doc-generator@2.2.2_eslint@9.29.0_jiti@2.6.1__typescript@5.9.2/node_modules/eslint-doc-generator/dist/node_modules:/home/manikandan-d/miq_prd_hub/n8n/node_modules/.pnpm/eslint-doc-generator@2.2.2_eslint@9.29.0_jiti@2.6.1__typescript@5.9.2/node_modules/eslint-doc-generator/node_modules:/home/manikandan-d/miq_prd_hub/n8n/node_modules/.pnpm/eslint-doc-generator@2.2.2_eslint@9.29.0_jiti@2.6.1__typescript@5.9.2/node_modules:/home/manikandan-d/miq_prd_hub/n8n/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/manikandan-d/miq_prd_hub/n8n/node_modules/.pnpm/eslint-doc-generator@2.2.2_eslint@9.29.0_jiti@2.6.1__typescript@5.9.2/node_modules/eslint-doc-generator/dist/bin/node_modules:/home/manikandan-d/miq_prd_hub/n8n/node_modules/.pnpm/eslint-doc-generator@2.2.2_eslint@9.29.0_jiti@2.6.1__typescript@5.9.2/node_modules/eslint-doc-generator/dist/node_modules:/home/manikandan-d/miq_prd_hub/n8n/node_modules/.pnpm/eslint-doc-generator@2.2.2_eslint@9.29.0_jiti@2.6.1__typescript@5.9.2/node_modules/eslint-doc-generator/node_modules:/home/manikandan-d/miq_prd_hub/n8n/node_modules/.pnpm/eslint-doc-generator@2.2.2_eslint@9.29.0_jiti@2.6.1__typescript@5.9.2/node_modules:/home/manikandan-d/miq_prd_hub/n8n/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../eslint-doc-generator/dist/bin/eslint-doc-generator.js" "$@"
else
  exec node  "$basedir/../eslint-doc-generator/dist/bin/eslint-doc-generator.js" "$@"
fi
